{
  "name": "tmi_frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start:local": "concurrently \"npm run start:mock:server --mockServerPort=4000\" \"ng serve -o\"",
    "start:development": "ng serve -o --configuration development",
    "start:mock:server": "npx json-server --middlewares mock-server/mock-middleware.js --watch mock-server/mock-server-db.config.json --routes mock-server/mock-server-routes.config.json --host localhost --port %npm_config_mockServerPort% --delay 2000",
    "build:development": "ng build --configuration development && npm run build:compress-replace-images && npm run build:copy-missing-files",
    "build:uat": "cross-env NODE_ENV=production ng build --configuration uat && npm run build:remove-uncompressed-files && npm run build:compress-replace-images && npm run build:copy-missing-files",
    "build:production": "cross-env NODE_ENV=production ng build --configuration production && npm run build:remove-uncompressed-files && npm run build:compress-replace-images && npm run build:copy-missing-files",
    "build:compress-replace-images": "node scripts/post-ng-build/compress-replace-images.mjs",
    "build:remove-uncompressed-files": "node scripts/post-ng-build/remove-uncompressed-files.mjs",
    "build:copy-missing-files": "sh scripts/post-ng-build/copyMissingFiles.sh",
    "test:local": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint --fix",
    "prettier:check": "npx prettier --check .",
    "prettier:apply": "npx prettier --write .",
    "update-version:commit": "sh scripts/versioning/updateProjectVersion.sh",
    "release-notes:commit": "sh scripts/versioning/finalizeReleaseNotes.sh",
    "tag-version-with-release-notes:commit": "sh scripts/versioning/tagVersionWithReleaseNotes.sh",
    "docker:build": "sh scripts/docker/buildImage.sh",
    "docker:start:development": "cross-env CMD_APP_VERSION=%npm_package_version% docker compose -f docker/docker-compose-development.yml --env-file docker/.env.development up -d",
    "docker:start:uat": "cross-env CMD_APP_VERSION=%npm_package_version% docker compose -f docker/docker-compose-uat.yml --env-file docker/.env.uat up -d",
    "docker:start:production": "cross-env CMD_APP_VERSION=%npm_package_version% docker compose -f docker/docker-compose-production.yml --env-file docker/.env.production up -d",
    "encrypt:file": "sh scripts/encryption/encryptFile.sh",
    "decrypt:file": "node scripts/encryption/decrypt-file.mjs",
    "generate:svg-sprite": "svg2sprite ./src/assets/svgs/icons src/assets/sprites/app-sprite.svg --stripAttrs stroke --stripAttrs id"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/google-maps": "^17.3.10",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@auth0/auth0-angular": "^2.2.1",
    "@ngrx/effects": "^17.0.1",
    "@ngrx/store": "^17.0.1",
    "@ngrx/store-devtools": "^17.0.1",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@protontech/openpgp": "^5.10.2",
    "@sentry/angular-ivy": "^7.92.0",
    "@types/uuid": "^9.0.7",
    "angular-oauth2-oidc": "^17.0.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "ng-zorro-antd": "^17.1.0",
    "ngx-material-timepicker": "^13.1.1",
    "process": "^0.11.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.3.9",
    "@angular-eslint/builder": "^17.1.1",
    "@angular-eslint/eslint-plugin": "^17.1.1",
    "@angular-eslint/eslint-plugin-template": "^17.1.1",
    "@angular-eslint/template-parser": "^17.1.1",
    "@angular/cli": "~17.3.9",
    "@angular/compiler-cli": "^17.3.12",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/crypto-js": "^4.1.2",
    "@types/dotenv-webpack": "^7.0.4",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "crypto-ts": "^1.0.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fs": "^0.0.1-security",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-overwrite": "^7.0.1",
    "jasmine-core": "~4.6.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "svg2sprite-cli": "^2.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2",
    "yargs": "^17.7.2"
  }
}
